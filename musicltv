//<![CDATA[
$(document).ready(function(){

	vol = getVolume();
	$("#main-player").jPlayer({
		ready: function (event) {
			$(this).jPlayer("setMedia", {

			});
		},
		swfPath: "https://itsongs.com/template/js",
		supplied: "mp3",
		wmode: "window",
		smoothPlayBar: true,
		keyEnabled: true,
		remainingDuration: true,
		toggleDuration: true,
		volume: vol *1
	});
});
function base64_decode(c){0<=c.indexOf("=")&&(c=c.substr(0,c.indexOf("=")));for(var k=0,d=0,b,l,e,g,f=0,a,h,m="";k<c.length;++k){l="="==c.charAt(k)?0:"ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/".indexOf(c.charAt(k));d=(d+6)%8;if(6!=d){b+=l>>d;if(0==f)g=!0,h=0,e=1,128>b&&(e=0,h=b&64,g=!1);else if(128!=(b&192))return!1;for(a=32;g&&0<a;a>>=1)b&a?++e:g=!1;g||(a=6+6*f-e,6<a&&(a=6),a&&(h+=b%(1<<a)<<6*(e-f)));f==e?(m+=String.fromCharCode(h),f=0):++f}b=d?l%(1<<d)<<8-d:0}return m}
function wait(ms){
   var start = new Date().getTime();
   var end = start;
   while(end < start + ms) {
     end = new Date().getTime();
  }
}
function nextTrack() {
    if ($('.track__item.track__item').length > 1) {
		var next = $('.track__item.track__item--active').nextAll(".track__item").eq(0)
		if ( !next.length ) next = $('.track__item:first');
		next.find('.track__play:first').click();
    }
}
function initYoutube() {
	if (!$('#yt_api_srcipt').length) {
	    var tag = document.createElement('script');
	    tag.id = "yt_api_srcipt";
	    tag.src = "https://www.youtube.com/iframe_api";
	    var firstScriptTag = document.getElementsByTagName('script')[0];
	    firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);
	 } else {
	 	onYouTubeIframeAPIReady();
	 }
}

function destroyYoutube()
{
	if (yt_player_1) {
		yt_player_1.destroy();
		yt_player_1 = null;
		clearInterval(youTubeInterval);

		$("#main-player").jPlayer({
			ready: function (event) {
				$(this).jPlayer("setMedia", {

				});
			},
			swfPath: "https://itsongs.com/template/js",
			supplied: "mp3",
			wmode: "window",
			smoothPlayBar: true,
			keyEnabled: true,
			remainingDuration: true,
			toggleDuration: true,
			volume: getVolume() *1
		});
	}
}

var yt_player_1,
	curtrack;
function onYouTubeIframeAPIReady(){
	yt_player_1 = new YT.Player('yt_player_1',{
		height		: '540',
		width		: '960',
		videoId		: curtrack.id,
		playerVars:{
			'autohide':			1,
			'autoplay':			1,
			'controls': 		0,
			'fs':				1,
			'disablekb':		0,
			'modestbranding':	1,
			// 'cc_load_policy': 1, // forces closed captions on
			'iv_load_policy':	3, // annotations, 1=on, 3=off
			// 'playlist': videoID, videoID, videoID, etc,
			'rel':				0,
			'showinfo':			0,
			'theme':			'dark',	// dark, light
			'color':			'red'	// red, white
			},
		events:{
			'onReady': onPlayerReady,
			'onPlaybackQualityChange': onPlayerPlaybackQualityChange,
			'onPlaybackRateChange' : onPlaybackRateChange,
			'onStateChange': onPlayerStateChange,
			'onError': onPlayerError
			}
		});
}

function onPlayerReady(data){
	initializeJplayerControls();
	yt_player_1.setVolume( getVolume()*100 );
	$('#jp_container_1 .jp-volume-bar .jp-volume-bar-value').width( getVolume()*100 + '%' );
} // END FUNCTION

function onPlayerPlaybackQualityChange(quality){
} // END FUNCTION

function onPlaybackRateChange(rate){
} // END FUNCTION

function onPlayerStateChange(state){
	switch(state.data){
		case -1: //unstarted
			/* do something */
			break;
		case 0: // ended
			$('#jp_container_1 .jp-pause').show();
			$('#jp_container_1 .jp-play').hide();
			nextTrack();
			break;
		case 1: // playing
			$('#jp_container_1 .jp-pause').show();
			$('#jp_container_1 .jp-play').hide();
			$('.track__item--active').find('.track__pause').css('display', 'inline-block');
			$('.track__item--active').find('.track__play').hide();
			startYoutubeTime();
			break;
		case 2: // paused
			$('#jp_container_1 .jp-pause').hide();
			$('#jp_container_1 .jp-play').show();
			$('.track__item--active').find('.track__pause').hide();
			$('.track__item--active').find('.track__play').css('display', 'inline-block');
			break;
		case 3: // buffering
			/* do something */
			break;
		case 5: // video cued
			/* do something */
			break;
		default:
			// do nothing
		}
} // END FUNCTION

function onPlayerError(error){
	console.log(error);
} // END FUNCTION

function youtubeFeedCallback(data){
	jQuery(document).ready(function(){
		$('#jp_container_1 .jp-title ul li').text( data.entry['title'].$t /* +' - from: '+data.entry["author"][0].name.$t */ );
		$('#jp_container_1 .jp-duration').text( Math.floor(data.entry['media$group']['yt$duration'].seconds/60)+':'+(data.entry['media$group']['yt$duration'].seconds%60) );
	});
} // END FUNCTION

var youTubeFrequency = 100;
var youTubeInterval = 0;
function startYoutubeTime(){
	if(youTubeInterval > 0) clearInterval(youTubeInterval);  // stop
	youTubeInterval = setInterval( "updateYoutubeTime()", youTubeFrequency );  // run
} // END FUNCTION
function updateYoutubeTime(){
	if ( yt_player_1 ) {
		if( yt_player_1.getCurrentTime()>=60 ){
			$('#jp_container_1 .jp-current-time').text( Math.floor(yt_player_1.getCurrentTime()/60)+':'+FormatNumberLength(Math.round(yt_player_1.getCurrentTime()%60),2) );
		}else{
			$('#jp_container_1 .jp-current-time').text( '0:'+FormatNumberLength(Math.round(yt_player_1.getCurrentTime()),2) );
		}
		$('#jp_container_1 .jp-progress .jp-play-bar').width( Math.round((yt_player_1.getCurrentTime()/yt_player_1.getDuration())*100)+'%' );
	}
} // END FUNCTION
function FormatNumberLength(num,length){var r=""+num;while(r.length<length){r="0"+r;}return r;} // END FUNCTION

function initializeJplayerControls(){
	$('#jp_container_1 .jp-pause').hide();
	$('#jp_container_1 .jp-unmute').hide();

	$('#jp_container_1 .jp-play').on('click',function(){
		$(this).hide();
		$('#jp_container_1 .jp-pause').show();
		if (yt_player_1) yt_player_1.playVideo();
	});
	$('#jp_container_1 .jp-pause').on('click',function(){
		$(this).hide();
		$('#jp_container_1 .jp-play').show();
		if (yt_player_1) yt_player_1.pauseVideo();
	});
	$('#jp_container_1 .jp-volume-bar').on('click', function(e){
		if (yt_player_1) {
			var posX = $(this).offset().left, posWidth = $(this).width();
			posX = ((e.pageX-posX)/posWidth) *100;
			// var posX = getVolume() *100;
			console.log(posX);
			$('#jp_container_1 .jp-volume-bar .jp-volume-bar-value').width( (posX)+'%' ).show();
			yt_player_1.setVolume(posX);
		}

		/*$('#jp_container_1 .jp-unmute').hide();
		$('#jp_container_1 .jp-mute').show();*/
	});
	$('#jp_container_1 .jp-seek-bar').click(function(e){
		if (yt_player_1) {
			var posX = $(this).offset().left, posWidth = $(this).width();
			posX = (e.pageX-posX)/posWidth;
			$('#jp_container_1 .jp-progress .jp-play-bar').width( (posX*100)+'%' );
			posX = Math.round((posX)*yt_player_1.getDuration());
			yt_player_1.seekTo(posX, true);
		}
	});
}

var isInitialized = false;
$(document).on('click', '.track__play', function(event) {
	var self = $(this);
	var container = self.parents('.track__item');

	self.hide();
	container.find('.track__pause').show();

	var pic = container.data('waveform'),
		id = container.data('id'),
		user = container.data('uid'),
		title = container.data('title');

	if ( container.hasClass('paused') ) {
		if ( 'yt_fk' == user ) {
			if (yt_player_1) yt_player_1.playVideo();
		} else {
			$("#main-player").jPlayer('play');
		}
		container.removeClass('paused').addClass('playing');
	} else {
		data_update(pic, id, user, title);
		container.addClass('track__item--active');
	}

});

$(document).on('click', '.track__pause', function(event) {
	var self = $(this);
	var container = self.parents('.track__item');

	container.removeClass('playing').addClass('paused');

	self.hide();
	container.find('.track__play').show();

	var pic = container.data('waveform'),
		id = container.data('id'),
		user = container.data('uid'),
		title = container.data('title');

	if ( 'yt_fk' == user ) {
		if (yt_player_1) yt_player_1.pauseVideo();
	} else {
		$("#main-player").jPlayer('pause');
	}

});

$(document).on($.jPlayer.event.play, "#main-player", function(event) {
	$('.track__item--active').find('.track__play').hide();
	$('.track__item--active').find('.track__pause').css('display', 'inline-block');
});

$(document).on($.jPlayer.event.pause, "#main-player", function(event) {
	$('.track__item--active').find('.track__play').css('display', 'inline-block');
	$('.track__item--active').find('.track__pause').hide();
});

$(document).on($.jPlayer.event.ended, "#main-player", function(event) {
	nextTrack();
});

function data_update(pic, id, user, title) { 
	$('.track__item').removeClass('track__item--active playing paused');

	$('.track__pause').hide();
	$('.track__play').css('display', 'inline-block');

	document.getElementById('change_bg').style.cssText = 'background: #333 url('+pic+'); background-size: 100% 42px; background-repeat: no-repeat;';
	$("#song_title").html(title);
	$("#song_phone_title").html(title);

	vol = getVolume();

    // fix for android
	if ( !isInitialized && $.jPlayer.platform.mobile ) {
		$("#main-player").jPlayer("setMedia", {
	        mp3: "https://itsongs.com/template/js/microtrack.mp3"
	    }).jPlayer("play");
	    if ( 'yt_fk' !== user ) wait(3000);
	    isInitialized = true;
	}
    // end of fix

    // add YT playback
    if ( 'yt_fk' !== user ) {
    	destroyYoutube();
    	$.ajax(
	    {
	        url : "https://itsongs.com/post.php?stream=true",
	        type: "POST",
	        data : "id="+id,
			cache: false,
			success: function(data) {
				$("#main-player").jPlayer("setMedia", {
			        mp3: data
			    }).jPlayer("play");
			}
	    });
    } else {
    	// YT playback
    	curtrack = {
    		id: base64_decode(id),
    		pic: pic,
    		title: title
    	};
    	$("#main-player").jPlayer('destroy');
    	if ( yt_player_1 ) {
    		yt_player_1.loadVideoById(curtrack.id)
    	} else {
    		initYoutube();
    	}
    }

    $.ajax(
    {
        url : "https://itsongs.com/post.php?stats=true",
        type: "POST",
        data : "id="+id+"&user="+user,
		cache: false,
		success: function(html) {

		}
    });
}
