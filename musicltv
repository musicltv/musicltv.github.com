//<![CDATA[
    $(document).ready(function(){
    $(".sub_search").submit(function(e){
    e.preventDefault();
    var search_query = $('input#dosearch').val();
	search_query = search_query.replace("/", "");
	search_query = search_query.split(" ").join("-");
    var search_url = "https://itsongs.com/search/"+search_query;

    pjax.invoke({
	    url:search_url, 
	   'container': 'content'
	    });	

    });
    });
    $(document).ready(function(){
    $(".sub_search_phone").submit(function(e){
    e.preventDefault();
    var search_query = $('input#dosearch_phone').val();
	search_query = search_query.replace("/", "");
	search_query = search_query.split(" ").join("-");
    var search_url = "https://itsongs.com/search/"+search_query;

    pjax.invoke({
	    url:search_url, 
	   'container': 'content'
	    });	

    });
    });
    // connect
    pjax.connect({
        'container': 'content',
		'useClass': 'pjaxer',
        'beforeSend': function(e){ },
        'complete': function(e){ 
		            reset();
				},
    });
